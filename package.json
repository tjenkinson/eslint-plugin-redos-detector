{
  "name": "eslint-plugin-redos-detector",
  "description": "An eslint plugin that detects vulnerable regex using \"https://github.com/tjenkinson/redos-detector\".",
  "main": "dist/eslint-plugin-redos-detector.js",
  "types": "dist/eslint-plugin-redos-detector.d.ts",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "regex",
    "redos",
    "pattern",
    "redos-detector"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjenkinson/eslint-plugin-redos-detector.git"
  },
  "author": "Tom Jenkinson <tom@tjenkinson.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tjenkinson/eslint-plugin-redos-detector/issues"
  },
  "homepage": "https://github.com/tjenkinson/eslint-plugin-redos-detector#readme",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && rollup --config rollup.config.js",
    "watch": "rollup --config rollup.config.js --watch",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "test": "jest",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "13.3.0",
    "@types/eslint": "8.4.3",
    "@types/jest": "28.1.1",
    "@types/node": "17.0.13",
    "eslint": "8.17.0",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "lint-staged": "13.0.2",
    "micromatch": "4.0.5",
    "prettier": "2.7.1",
    "redos-detector": "3.1.3",
    "rollup": "2.75.6",
    "rollup-plugin-typescript2": "0.32.1",
    "ts-jest": "28.0.5",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "eslint": ">=6"
  }
}
